{
    "name": "@wuchale/jsx",
    "version": "0.9.7",
    "description": "Protobuf-like i18n from plain code: JSX adapter",
    "scripts": {
        "dev": "tsc --watch",
        "build": "tsc",
        "test": "node tests/index.ts"
    },
    "keywords": [
        "i18n",
        "internationalization",
        "translation",
        "gettext",
        "jsx",
        "react",
        "preact",
        "solidjs",
        "vite",
        "po",
        "react-i18n",
        "vite-plugin",
        "compile-time",
        "ast",
        "gemini",
        "translation-tooling",
        "multilingual",
        "localization",
        "l10n",
        "lingui",
        "automatic-i18n",
        "lightweight"
    ],
    "files": [
        "dist",
        "src/loaders"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./runtime.jsx": {
            "types": "./dist/runtime.d.ts",
            "default": "./dist/runtime.jsx"
        },
        "./runtime.solid.jsx": {
            "types": "./dist/runtime.solid.d.ts",
            "default": "./dist/runtime.solid.jsx"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wuchalejs/wuchale.git"
    },
    "homepage": "https://wuchale.dev",
    "bugs": "https://github.com/wuchalejs/wuchale/issues",
    "author": "K1DV5",
    "license": "MIT",
    "dependencies": {
        "@sveltejs/acorn-typescript": "^1.0.8",
        "acorn": "^8.15.0",
        "magic-string": "^0.30.21",
        "wuchale": "^0.18.8"
    },
    "devDependencies": {
        "@types/estree-jsx": "^1.0.5",
        "typescript": "^5.9.3"
    },
    "type": "module"
}
