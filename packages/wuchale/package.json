{
    "name": "wuchale",
    "version": "0.17.5",
    "description": "Protobuf-like i18n from plain code",
    "scripts": {
        "dev": "tsc --watch",
        "build": "tsc",
        "test": "node tests"
    },
    "keywords": [
        "i18n",
        "internationalization",
        "translation",
        "gettext",
        "vite",
        "po",
        "vite-plugin",
        "compile-time",
        "ast",
        "gemini",
        "translation-tooling",
        "multilingual",
        "localization",
        "l10n",
        "lingui",
        "automatic-i18n",
        "lightweight"
    ],
    "files": [
        "dist",
        "src/adapter-vanilla/loaders"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./virtual": "./src/virtual.d.ts",
        "./adapter-vanilla": {
            "types": "./dist/adapter-vanilla/index.d.ts",
            "default": "./dist/adapter-vanilla/index.js"
        },
        "./runtime": {
            "types": "./dist/runtime.d.ts",
            "default": "./dist/runtime.js"
        },
        "./load-utils": {
            "types": "./dist/load-utils/index.d.ts",
            "default": "./dist/load-utils/index.js"
        },
        "./load-utils/server": {
            "types": "./dist/load-utils/server.d.ts",
            "default": "./dist/load-utils/server.js"
        },
        "./load-utils/pure": {
            "types": "./dist/load-utils/pure.d.ts",
            "default": "./dist/load-utils/pure.js"
        },
        "./adapter-utils": {
            "types": "./dist/adapter-utils/index.d.ts",
            "default": "./dist/adapter-utils/index.js"
        },
        "./url": {
            "types": "./dist/url.d.ts",
            "default": "./dist/url.js"
        }
    },
    "bin": {
        "wuchale": "./dist/cli/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wuchalejs/wuchale.git"
    },
    "homepage": "https://wuchale.dev",
    "bugs": "https://github.com/wuchalejs/wuchale/issues",
    "author": "K1DV5",
    "license": "MIT",
    "dependencies": {
        "@sveltejs/acorn-typescript": "^1.0.6",
        "acorn": "^8.15.0",
        "chokidar": "^4.0.3",
        "magic-string": "^0.30.19",
        "path-to-regexp": "^8.3.0",
        "picomatch": "^4.0.3",
        "pofile": "^1.1.4",
        "tinyglobby": "^0.2.15"
    },
    "devDependencies": {
        "@types/node": "^24.8.1",
        "@types/picomatch": "^4.0.1",
        "typescript": "^5.9.3"
    },
    "type": "module"
}
