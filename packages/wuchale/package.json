{
    "name": "wuchale",
    "version": "0.9.6",
    "description": "i18n from normal code",
    "scripts": {
        "dev": "tsc --watch",
        "build": "tsc",
        "test": "node tests"
    },
    "keywords": [
        "i18n",
        "internationalization",
        "translation",
        "gettext",
        "vite",
        "po",
        "vite-plugin",
        "compile-time",
        "ast",
        "gemini",
        "translation-tooling",
        "multilingual",
        "localization",
        "l10n",
        "lingui",
        "automatic-i18n",
        "lightweight"
    ],
    "files": [
        "dist",
        "src"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./virtual": {
            "types": "./src/virtual.d.ts"
        },
        "./config": {
            "types": "./dist/config.d.ts",
            "default": "./dist/config.js"
        },
        "./handler": {
            "types": "./dist/handler.d.ts",
            "default": "./dist/handler.js"
        },
        "./adapters": {
            "types": "./dist/adapters.d.ts",
            "default": "./dist/adapters.js"
        },
        "./adapter-vanilla": {
            "types": "./dist/adapter-vanilla.d.ts",
            "default": "./dist/adapter-vanilla.js"
        },
        "./runtime": {
            "types": "./dist/runtime.d.ts",
            "default": "./dist/runtime.js"
        },
        "./run-client": {
            "types": "./dist/run-client.d.ts",
            "default": "./dist/run-client.js"
        },
        "./run-server": {
            "types": "./dist/run-server.d.ts",
            "default": "./dist/run-server.js"
        }
    },
    "bin": {
        "wuchale": "./dist/cli.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wuchalejs/wuchale.git"
    },
    "homepage": "https://wuchale.dev",
    "bugs": "https://github.com/wuchalejs/wuchale/issues",
    "author": "K1DV5",
    "license": "MIT",
    "dependencies": {
        "acorn": "^8.15.0",
        "@sveltejs/acorn-typescript": "^1.0.5",
        "magic-string": "^0.30.17",
        "picomatch": "^4.0.3",
        "pofile": "^1.1.4",
        "tinyglobby": "^0.2.14"
    },
    "devDependencies": {
        "@types/node": "^24.1.0",
        "@types/picomatch": "^4.0.1",
        "typescript": "^5.8.3"
    },
    "type": "module"
}
