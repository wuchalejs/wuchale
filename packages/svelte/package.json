    "author": "K1DV5",
    "license": "MIT",
    "dependencies": {
        "wuchale": "^0.9.0",
        "svelte": "^5.28.1"
    },
    "devDependencies": {
        "typescript": "^5.8.3"
    },
    "type": "module"
