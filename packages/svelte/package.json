{
    "name": "@wuchale/svelte",
    "version": "0.9.0",
    "description": "i18n from normal svelte code",
    "scripts": {
        "dev": "tsc --watch",
        "build": "tsc",
        "test": "node tests"
    },
    "keywords": [
        "i18n",
        "internationalization",
        "translation",
        "gettext",
        "svelte",
        "sveltekit",
        "vite",
        "po",
        "svelte-i18n",
        "vite-plugin",
        "compile-time",
        "ast",
        "gemini",
        "translation-tooling",
        "multilingual",
        "localization",
        "l10n",
        "lingui",
        "automatic-i18n",
        "lightweight",
        "svelte-plugin"
    ],
    "files": [
        "dist",
        "src"
    ],
    "exports": {
